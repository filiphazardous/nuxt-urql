{
  "name": "@bicou/nuxt-urql",
  "version": "1.1.2",
  "description": "Nuxt URQL module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gbicou/nuxt-urql.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxi prepare playground",
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "dev:build": "nuxi build playground",
    "dev:start": "nuxi start playground",
    "build": "nuxt-module-build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clean": "rimraf dist/ playground/.nuxt/ playground/.output/ playground/node_modules/.vite/",
    "release": "semantic-release"
  },
  "dependencies": {
    "@nuxt/kit": "^3.1.1",
    "@urql/core": "^3.1.1",
    "@urql/vue": "^1.0.4"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.1.1",
    "@nuxt/test-utils": "^3.1.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "changelogen": "^0.4.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql": "^16.6.0",
    "nuxt": "^3.1.1",
    "prettier": "^2.8.3",
    "semantic-release": "^20.1.0",
    "vitest": "^0.28.3",
    "vue": "^3.2.47",
    "@bicou/nuxt-urql-sample-schema": "workspace:*"
  },
  "peerDependencies": {
    "graphql": "^16.6.0",
    "vue": "^3.2.47"
  },
  "packageManager": "pnpm@7.26.3",
  "publishConfig": {
    "access": "public"
  }
}
