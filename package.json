{
  "name": "@bicou/nuxt-urql",
  "version": "1.9.3",
  "description": "Nuxt URQL module",
  "license": "MIT",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gbicou/nuxt-urql.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxi prepare playground",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "dev:build": "nuxi build playground",
    "dev:start": "nuxi start playground",
    "build": "nuxt-module-build build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clean": "rimraf dist/ playground/.nuxt/ playground/.output/ playground/node_modules/.vite/",
    "release": "changeset publish"
  },
  "dependencies": {
    "@urql/core": "^5.1.0",
    "@urql/vue": "^1.4.2"
  },
  "devDependencies": {
    "@nuxt/kit": "3.15.4",
    "@bicou/countries-server-schema": "1.7.0",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.12",
    "@nuxt/module-builder": "0.8.4",
    "@nuxt/schema": "3.15.4",
    "@nuxt/test-utils": "3.15.4",
    "@types/node": "22.13.2",
    "@urql/exchange-execute": "2.3.0",
    "defu": "6.1.4",
    "eslint": "9.20.1",
    "graphql": "16.10.0",
    "nuxt": "3.15.4",
    "typescript": "5.6.3",
    "vitest": "3.0.5"
  },
  "peerDependencies": {
    "defu": "^6.1.4",
    "graphql": "^16.10.0",
    "vue": "^3.5.13"
  },
  "packageManager": "pnpm@10.4.0+sha512.6b849d0787d97f8f4e1f03a9b8ff8f038e79e153d6f11ae539ae7c435ff9e796df6a862c991502695c7f9e8fac8aeafc1ac5a8dab47e36148d183832d886dd52",
  "publishConfig": {
    "access": "public"
  }
}
